{
  "name": "electron-es6-react",
  "version": "0.1.0",
  "main": "main.js",
  "license": "MIT",
  "author": "Michael Michailidis",
  "homepage": "./",
  "repository": "b52/electron-es6-react",
  "description": "This is a convertion tool from RAF (Fugi Raw Format) to PNG with Resizing",
  "scripts": {
    "start": "electron -r babel-register .",
    "package": "build --em.main=build/main.js",
    "postinstall": "electron-rebuild"
  },
  "dependencies": {
    "antd": "^3.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-register": "^6.3.13",
    "image-size": "^0.6.2",
    "libraw": "^0.1.4",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-dropzone": "^4.2.8",
    "sharp": "^0.19.0"
  },
  "devDependencies": {
    "electron": "^1.4.3",
    "electron-rebuild": "^1.7.3"
  },
  "build": {
    "appId": "com.karmadust.mancuspianconvert",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories":{
      "buildResources": "assets"
    }
  }
}
